                android:textSize="10dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/mark4"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="Pass"
                android:textSize="10dp" />

            <View
                android:id="@+id/line5"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:layout_below="@+id/subDis4"
                android:layout_marginEnd="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/line" />

            <TextView
                android:id="@+id/subcode5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/line5"
                android:layout_marginLeft="10dp"
                android:padding="2dp"
                android:text="IT6001"
                android:textColor="#000000"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/mark5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/subcode5"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="22"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/subDis5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/subcode5"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:padding="5dp"
                android:text="Service Oriented Architecture"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/mark5"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="Pass"
                android:textSize="10dp" />

        </RelativeLayout>
    </android.support.v7.widget.CardView>


</RelativeLayout>
