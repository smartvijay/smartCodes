<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.abishek.single_user.Exam_test">

    <android.support.v7.widget.CardView
        android:id="@+id/card_heading"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        app:cardCornerRadius="10dp">

        <TextView
            android:id="@+id/header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@color/cardview_shadow_start_color"
            android:gravity="center"
            android:text="Slip Test 1"
            android:textColor="#000000"
            android:textSize="20dp" />


    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/card_heading"
        android:layout_margin="3dp"
        app:cardCornerRadius="10dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/subcode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:padding="2dp"
                android:text="CS6701"
                android:textColor="#000000"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/acc_no"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/subcode"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:padding="5dp"
                android:text="Cripto Graphy and Network Security"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/mark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="25"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/res_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/mark"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="Pass"
                android:textSize="10dp" />

            <View
                android:id="@+id/line1"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:layout_below="@+id/acc_no"
                android:layout_marginEnd="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/line" />


            <TextView
                android:id="@+id/subcode1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/line1"
                android:layout_marginLeft="10dp"
                android:padding="2dp"
                android:text="CS6702"
                android:textColor="#000000"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/mark1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/subcode1"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="20"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/subDis1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/subcode1"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:padding="5dp"
                android:text="Graph Theory and Application"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/mark1"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="Pass"
                android:textSize="10dp" />

            <View
                android:id="@+id/line2"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:layout_below="@+id/subDis1"
                android:layout_marginEnd="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/line" />

            <TextView
                android:id="@+id/subcode2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/line2"
                android:layout_marginLeft="10dp"
                android:padding="2dp"
                android:text="CS6703"
                android:textColor="#000000"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/mark2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/subcode2"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="30"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/subDis2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/subcode2"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:padding="5dp"
                android:text="Grid and Cloud Computing"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/mark2"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="Pass"
                android:textSize="10dp" />

            <View
                android:id="@+id/line3"
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:layout_below="@+id/subDis2"
                android:layout_marginEnd="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="20dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/line" />

            <TextView
                android:id="@+id/subcode3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/line3"
                android:layout_marginLeft="10dp"
                android:padding="2dp"
                android:text="CS6704"
                android:textColor="#000000"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/mark3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/subcode3"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:padding="5dp"
                android:text="21"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/subDis3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
